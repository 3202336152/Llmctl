{
  "name": "llmctl",
  "version": "1.0.0",
  "description": "A CLI tool for switching between different LLM configurations in Claude Code environment",
  "keywords": [
    "llm",
    "claude",
    "cli",
    "ai",
    "anthropic",
    "typescript"
  ],
  "author": "llmctl",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "llmctl": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test:run && npm run lint && npm run typecheck"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "cross-spawn": "^7.0.3",
    "inquirer": "^10.2.2",
    "ora": "^8.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.6",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@vitest/coverage-v8": "^2.1.2",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/3202336152/llmctl.git"
  },
  "bugs": {
    "url": "https://github.com/3202336152/llmctl/issues"
  },
  "homepage": "https://github.com/3202336152/llmctl#readme"
}
